<?xml version="1.0" encoding="UTF-8"?>

<composite xmlns="http://www.osoa.org/xmlns/sca/1.0"
  xmlns:frascati="http://frascati.ow2.org/xmlns/sca/1.1"
  targetNamespace="http://frascati.ow2.org/calcularpi"

  name="ServerRMI">

  <service name="r" promote="server/r">
    <interface.java interface="java.lang.Runnable" />
  </service>

  <component name="server">
    <property name="myServiceuri">rmi://localhost:1090/generarPuntos</property>

    <service name="r">
      <interface.java interface="java.lang.Runnable" />
    </service>
    
    <service name="generarPuntos" promote="server/generarPuntos">
      <interface.java interface="org.ow2.frascati.calculopiv1JimenezMartinezFernandez.services.ServicioGenerarPuntos"></interface.java>
      <frascati:binding.rmi host="localhost" serviceName="rmiService12" port="1090" />
	  </service>

    <reference name="servicioComBroker" promote="server/servicioComBroker">
      <frascati:binding.rmi host="localhost"
        serviceName="rmiService33" port="1098"/>
    </reference>

    <implementation.java class="org.ow2.frascati.calculopiv1JimenezMartinezFernandez.Server" />

  </component>

</composite>
